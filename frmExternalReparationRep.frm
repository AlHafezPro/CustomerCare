Attribute VB_Name = "frmExternalReparationRep"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim externalReparationPaymentTypeRec As ExternalReparationType

Private Sub Form_Load()
init
End Sub


Sub init()
    top = 0
    left = 0
    FillCombos
End Sub

Sub FillCombos()
    Dim rsPayment As New ADODB.Recordset
    sqlText = "Select No , Name  From PayMethod "
    Set rsPayment = de.con.Execute(sqlText)
    Set ComboPayment.RowSource = rsPayment
    ComboPayment.listField = "Name"
    ComboPayment.BoundColumn = "No"
    ComboPayment.BoundText = 0
End Sub

Friend Sub GetExternalReparationType()


If Not IsDate(TxtFromDate.Text) Or Not IsDate(TxtToDate.Text) Or Not ComboPayment.MatchedWithList Then
    externalReparationPaymentTypeRec.FromDate = Null
    externalReparationPaymentTypeRec.TillDate = Null
    externalReparationPaymentTypeRec.PaymenrTypeMethod = Null
    Exit Sub
End If

With externalReparationPaymentTypeRec
    .FromDate = TxtFromDate.Text
    .TillDate = TxtToDate.Text
    .PaymenrTypeMethod = ComboPayment.BoundText
End With

End Sub

Friend Function ExeccProcedure(withDataOption As Integer) As Boolean

On Error GoTo errorhandler
Dim sqlText As String
GetExternalReparationType
    
If IsNull(externalReparationPaymentTypeRec.FromDate) Or IsNull(externalReparationPaymentTypeRec.TillDate) Or IsNull(externalReparationPaymentTypeRec.PaymenrTypeMethod) Then
    ExeccProcedure = False
Else
   With externalReparationPaymentTypeRec
        sqlText = "ADHAMoUTMaint '" & ConvertControlDate(.FromDate) & "','" & ConvertControlDate(.TillDate) & "'," & .PaymenrTypeMethod & "," & withDataOption
        de.con.Execute (sqlText)
    End With
    ExeccProcedure = True
End If
Exit Function
errorhandler:
ExeccProcedure = False
MsgBox Err.Description

End Function

Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
Select Case Button.Index
    Case 1
        If ExeccProcedure(1) Then
            PrintRep 1
        End If
    Case 3
        Unload Me
End Select
End Sub

Sub PrintRep(Voption As Integer)
On Error GoTo errorhandler
Screen.MousePointer = vbHourglass
With cr1
    .Connect = ConnectName("")
        .Formulas(0) = "fdate=" & "'" & externalReparationPaymentTypeRec.FromDate & "'"
        .Formulas(1) = "ldate=" & "'" & externalReparationPaymentTypeRec.TillDate & "'"
        .SQLQuery = "SELECT PieceStockNo,PieceName , Qty,Price,AccNo,Cash,Name , TotalWagesByDateAndCash from AdhamOutMaintRep "
        Select Case Voption
        Case 1
            .SQLQuery = .SQLQuery & " Order By  Cash,PieceStockNo ASC"
            .ReportFileName = App.Path & "\Reports\adhamoutmaint.rpt"
        Case 2
            .SQLQuery = .SQLQuery & " Order By  regestdate,PieceStockNo ASC"
            .ReportFileName = App.Path & "\Reports\_adhamoutmaint.rpt"
        End Select
        .DiscardSavedData = True
        .WindowState = crptMaximized
        .Action = 1
End With
Screen.MousePointer = vbDefault
Exit Sub
errorhandler:
MsgBox Err.Description
Screen.MousePointer = vbDefault
End Sub

Private Sub Toolbar1_ButtonMenuClick(ByVal ButtonMenu As MSComctlLib.ButtonMenu)
Select Case ButtonMenu.Tag
    Case 1 'Summary
        If ExeccProcedure(1) Then
            PrintRep 1
        End If
    Case 2 'Details
        If ExeccProcedure(0) Then
            PrintRep 2
        End If
    End Select
End Sub

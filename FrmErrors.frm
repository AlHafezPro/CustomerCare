Attribute VB_Exposed = False
Const colCash = 1
Const colcashtype = 2
Const colModNo = 3
Const colSymbol = 4
Const colProdFamNameA = 5
Const colREpTYpeDescription = 6
Const colCountRec = 7

Sub FillFormating(ByVal i As Integer)
If i = 1 Then
    fs = "|>" + "Cash"
    fs = fs + "|>" + "äæÚ ÇáÚØá"
    fs = fs + "|>" + "ÑÞã ÇáãæÏíá"
    fs = fs + "|>" + "ÅÓã ÇáãæÏíá"
    fs = fs + "|>" + "ÇáÚÇÆáÉ"
    fs = fs + "|>" + "ÇáÚØá"
    fs = fs + "|>" + "ÇáÚÏÏ"
    With FlexGrid
        .FormatString = fs
        .Cols = 8
        .ColWidth(colCash) = 0
        SetColWidths colcashtype, FlexGrid
        .ColWidth(colModNo) = 0
        SetColWidths colSymbol, FlexGrid
        SetColWidths colProdFamNameA, FlexGrid
        SetColWidths colREpTYpeDescription, FlexGrid
        SetColWidths colCountRec, FlexGrid
    End With
End If
End Sub

Sub SetColWidths(ByVal ColNo As Integer, FlexGrid As VSFlexGrid)
    With FlexGrid
        .AutoSize (ColNo)
    End With
End Sub

Function ExecProcedure() As Boolean
    On Error GoTo ErrorHandler
    sqlText = "exec sp_ReparationErrorsByModels '" & ConvertControlDate(TxtFromDate.Text) & "','" & ConvertControlDate(TxtTillDate.Text) & "'"
    de.con.Execute (sqlText)
    ExecProcedure = True
    Exit Function
ErrorHandler:
   MsgBox Err.Description
   ExecProcedure = False
End Function

Sub PrintRep()
On Error GoTo ErrorHandler
With cr1
    .Connect = ConnectName("")
    .Formulas(0) = "FromDate='" & TxtFromDate.Text & "'"
    .Formulas(1) = "TillDate='" & TxtTillDate.Text & "'"
    .ReportFileName = App.Path + "\Reports\RepErrors.rpt"
    .SQLQuery = "select Cash, cashtype, modno, Symbol, ProdFamNameA, REpTYpeDescription, CountRec from t_ReparationErrors Order By ProdFamNameA asc , CountREc DEsc , ModNo Asc"
    .DiscardSavedData = True
    .WindowState = crptMaximized
    .Action = 1
End With
Exit Sub
ErrorHandler:
   MsgBox Err.Description
End Sub

Private Sub Form_Load()
    top = 0
    left = 0
    FlexGrid.Rows = 1
    FillFormating 1
    TxtFromDate.Text = "01/01/" + LTrim(RTrim(Year(Now)))
    TxtTillDate.Text = "31/12/" + LTrim(RTrim(Year(Now)))
    LCount.Caption = ""
End Sub

Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
Dim rs As New ADODB.Recordset
Select Case Button.Index
    Case 1
        If ExecProcedure Then
            sqlText = "select Cash, cashtype, modno, Symbol, ProdFamNameA, REpTYpeDescription, CountRec from t_ReparationErrors Order BY  ProdFamNameA asc,ModNo Asc"
            Set rs = de.con.Execute(sqlText)
            Set FlexGrid.DataSource = rs
            LCount.Caption = rs.RecordCount
            FillFormating 1
        End If
    Case 3
        PrintRep
    Case 5
        Unload Me
End Select
End Sub

